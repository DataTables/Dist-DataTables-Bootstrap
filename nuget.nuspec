<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>1.12.1</version>
        <id>datatables.net-bs</id>
        <description>This is DataTables with styling for [Bootstrap](https://getbootstrap.com/docs/3.3/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Bootstrap.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>Bootstrap  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net" version="1.11.3"/>
            <dependency id="bootstrap" version="3.*"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/dataTables.bootstrap.js" target="content\Scripts"/>
        <file src="js/dataTables.bootstrap.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/dataTables.bootstrap.min.js" target="content\Scripts"/>
        <file src="js/dataTables.bootstrap.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/dataTables.bootstrap.css" target="content\Content"/>
        <file src="css/dataTables.bootstrap.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/dataTables.bootstrap.min.css" target="content\Content"/>
        <file src="css/dataTables.bootstrap.min.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="types/dataTables.bootstrap.d.ts" target="content\Scripts"/>
        <file src="types/dataTables.bootstrap.d.ts" target="contentFiles\any\any\wwwroot\js"/>
    </files>
</package>

